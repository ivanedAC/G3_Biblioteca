	<queryString>
		<![CDATA[select dr.isbn, lb.nombre, count(dr.isbn)
from detalle_reserva dr inner join reserva rv on dr.cod_reserva = rv.codigo
inner join libro lb on dr.isbn = lb.isbn
group by 1, 2
order by 2 asc
limit $P{parCantidad}]]>
