	<queryString>
		<![CDATA[select per.nombres as proveedor, ejp.isbn, lb.nombre as libro, dp.precio, op.sub_total
from orden_compra op inner join detalle_compra dp on op.num_orden = dp.num_orden
inner join proveedor prv on op.cod_proveedor = prv.codigo
inner join persona per on prv.cod_persona = per.codigo
inner join ejemplar ejp on dp.ejemplarcodigo = ejp.codigo
inner join libro lb on ejp.isbn = lb.isbn]]>
	</queryString>
